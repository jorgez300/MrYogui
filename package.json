{
  "name": "mryogui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:clean": "docker stop MrYogui && docker rm MrYogui && docker rmi mryogui-image",
    "docker:build": "docker build -t mryogui-image . && docker run --name MrYogui -p 22001:3000 mryogui-image",
    "docker:run": "docker start MrYogui",
    "docker:start": "next build && next start"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
